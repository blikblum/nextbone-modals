{
  "name": "nextbone-modals",
  "version": "0.11.1",
  "description": "Modal service for Nextbone",
  "main": "dist/nextbone-modals.js",
  "module": "dist/nextbone-modals.js",
  "scripts": {
    "test": "jest",
    "build": "node tools/build",
    "coverage": "jest --coverage",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/blikblum/nextbone-modals.git"
  },
  "keywords": [
    "modal",
    "marionette",
    "bootstrap",
    "backbone"
  ],
  "author": "Luiz Américo Pereira Câmara",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/blikblum/nextbone-modals/issues"
  },
  "homepage": "https://github.com/blikblum/nextbone-modals",
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "babel-jest": "^24.8.0",
    "del": "^4.1.1",
    "jest": "^24.8.0",
    "jquery": "^3.4.1",
    "nextbone": "^0.12.0",
    "rollup": "^1.12.2",
    "rollup-plugin-babel": "^4.3.2",
    "standard": "^12.0.1",
    "underscore": "^1.9.1"
  },
  "peerDependencies": {
    "jquery": "^3.3.1",
    "nextbone": "^0.12.0",
    "underscore": "^1.9.1"
  },
  "dependencies": {},
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "browsers": [
              "chrome 60"
            ]
          },
          "modules": false
        }
      ]
    ],
    "env": {
      "test": {
        "presets": [
          [
            "@babel/preset-env",
            {
              "targets": {
                "node": "current"
              }
            }
          ]
        ]
      }
    }
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/"
    ],
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!nextbone).+\\.js$"
    ]
  },
  "standard": {
    "env": {
      "browser": true
    }
  }
}
