{
  "name": "nextbone-modals",
  "version": "0.14.0",
  "description": "Modal service for Nextbone",
  "main": "dist/nextbone-modals.js",
  "module": "dist/nextbone-modals.js",
  "scripts": {
    "start": "web-dev-server --open example/src/ --node-resolve",
    "start:watch": "web-dev-server --open example/src/ --node-resolve --watch",
    "test": "jest",
    "build": "node tools/build",
    "coverage": "jest --coverage",
    "format": "prettier src/**/*.js --write",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/blikblum/nextbone-modals.git"
  },
  "keywords": [
    "modal",
    "nextbone",
    "bootstrap",
    "backbone",
    "web-component"
  ],
  "author": "Luiz Américo Pereira Câmara",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/blikblum/nextbone-modals/issues"
  },
  "homepage": "https://github.com/blikblum/nextbone-modals",
  "devDependencies": {
    "@babel/core": "^7.14.2",
    "@babel/plugin-proposal-decorators": "^7.14.2",
    "@babel/preset-env": "^7.14.2",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@testing-library/jest-dom": "^5.12.0",
    "@web/dev-server": "^0.1.17",
    "@web/dev-server-rollup": "^0.3.3",
    "babel-jest": "^26.6.3",
    "bootstrap": "4",
    "del": "^5.1.0",
    "jest": "^26.6.3",
    "jquery": "^3.6.0",
    "lodash-es": "^4.17.21",
    "nextbone": "^0.21.1",
    "popper.js": "1",
    "prettier": "^2.3.0",
    "rollup": "^2.48.0",
    "rollup-plugin-babel": "^4.4.0"
  },
  "peerDependencies": {
    "jquery": "^3.4.1",
    "nextbone": "^0.20.0"
  },
  "dependencies": {},
  "jest": {
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/"
    ],
    "transformIgnorePatterns": [],
    "setupFilesAfterEnv": [
      "<rootDir>/test/unit/setup.js"
    ]
  },
  "standard": {
    "env": {
      "browser": true
    }
  }
}
