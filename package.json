{
  "name": "nextbone-modals",
  "version": "0.16.0",
  "description": "Modal service for Nextbone",
  "type": "module",
  "main": "dist/nextbone-modals.js",
  "module": "dist/nextbone-modals.js",
  "workspaces": [
    "examples/*"
  ],
  "scripts": {
    "test": "yarn node --experimental-vm-modules $(yarn bin jest)",
    "build": "node tools/build.cjs",
    "coverage": "jest --coverage",
    "format": "prettier src/**/*.js --write",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/blikblum/nextbone-modals.git"
  },
  "keywords": [
    "modal",
    "nextbone",
    "bootstrap",
    "backbone",
    "web-component"
  ],
  "author": "Luiz Américo Pereira Câmara",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "bugs": {
    "url": "https://github.com/blikblum/nextbone-modals/issues"
  },
  "homepage": "https://github.com/blikblum/nextbone-modals",
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@testing-library/jest-dom": "^5.16.5",
    "bootstrap": "5.2.3",
    "del": "^5.1.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jquery": "^3.6.0",
    "lodash-es": "^4.17.21",
    "nextbone": "^0.24.1",
    "popper.js": "1.16.1",
    "prettier": "^2.2.1",
    "rollup": "^2.26.8"
  },
  "peerDependencies": {
    "jquery": "^3.6.0",
    "nextbone": "^0.24.1"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/"
    ],
    "transformIgnorePatterns": [],
    "setupFilesAfterEnv": [
      "<rootDir>/test/unit/setup.js"
    ]
  },
  "packageManager": "yarn@3.1.1"
}