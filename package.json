{
  "name": "nextbone-modals",
  "version": "1.0.0",
  "description": "Modal service for Nextbone",
  "type": "module",
  "main": "src/index.js",
  "module": "src/index.js",
  "types": "types/index.d.ts",
  "workspaces": [
    "examples/*"
  ],
  "scripts": {
    "test": "yarn node --experimental-vm-modules $(yarn bin jest)",
    "coverage": "jest --coverage",
    "format": "prettier src/**/*.js --write",
    "types": "tsc --project tsconfig.types.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/blikblum/nextbone-modals.git"
  },
  "keywords": [
    "modal",
    "nextbone",
    "bootstrap",
    "backbone",
    "web-component"
  ],
  "author": "Luiz Américo Pereira Câmara",
  "license": "MIT",
  "files": [
    "src",
    "types"
  ],
  "bugs": {
    "url": "https://github.com/blikblum/nextbone-modals/issues"
  },
  "homepage": "https://github.com/blikblum/nextbone-modals",
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "bootstrap": "5.2.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jquery": "^3.6.0",
    "lodash-es": "^4.17.21",
    "nextbone": "^0.27.3",
    "popper.js": "1.16.1",
    "prettier": "^2.2.1",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "jquery": "^3.6.0",
    "nextbone": "^0.24.1"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/"
    ],
    "transformIgnorePatterns": [],
    "setupFilesAfterEnv": [
      "<rootDir>/test/unit/setup.js"
    ]
  },
  "packageManager": "yarn@3.1.1"
}